        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/Green_level2</item>
        <item name="colorPrimaryVariant">@color/buttons_color</item>
        <item name="colorOnPrimary">@color/Green_level3</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/Green_level1</item>
        <item name="colorSecondaryVariant">@color/buttons_color</item>
        <item name="colorOnSecondary">@color/backgroud_white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorSecondary</item>
        <!-- Customize your theme here. -->
    </style>




